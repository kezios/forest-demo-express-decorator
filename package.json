{
  "name": "forest-demo-express-decorator",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server.js",
    "build": "rm -rf ./dist && npm run compile && npm run cp-config",
    "start:dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "compile": "tsc",
    "lint": "eslint . -c .eslintrc.json --ext .ts",
    "cp-config": "cp .env ./dist/ && cp -R public ./dist/"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "chalk": "1.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^7.7.2",
    "forest-express-sequelize": "^8.5.7",
    "morgan": "^1.10.0",
    "pg": "~8.2.2",
    "require-all": "^3.0.0",
    "sequelize": "^5.15.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.39",
    "@types/sequelize": "^4.28.13",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.3"
  }
}
